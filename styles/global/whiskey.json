		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Light",
					"slug": "light",
					"shadow": "0 8px 12px -4px rgb(0 0 0 / 30%);"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"shadow": "0 6px 7px -1px rgb(0 0 0 / 50%);"
				},
				{
					"name": "Diffused",
					"slug": "diffused",
					"shadow": "0 12px 20px -7px rgba(0,0,0,.4)"	
				}
			]
		},
		"custom": {	
			"linkColor": "var(--wp--preset--color--primary)",
			"linkHoverColor": "var(--wp--preset--color--secondary)",
			"linkTextDecoThickness": "1px",
			"navMainColor": "var(--wp--preset--color--primary)",
			"navMainHoverColor": "var(--wp--preset--color--secondary)",
			"navMainSubColor": "var(--wp--preset--color--base)",
			"navMainSubHoverColor": "var(--wp--preset--color--alternate)",
			"navDarkOverLightColor": "var(--wp--preset--color--contrast)",
			"navDarkOverLightHoverColor": "var(--wp--preset--color--secondary)",
			"navLightOverDarkColor": "var(--wp--preset--color--base)",
			"navLightOverDarkHoverColor": "var(--wp--preset--color--neutral-light)",
			"eyebrowSize": "var(--wp--preset--font-size--small)",
			"eyebrowColor": "var(--wp--preset--color--neutral)",
			"eyebrowFont": "var(--wp--preset--font-family--display)",
			"eyebrowTextTransform": "uppercase",
			"creativeSize": "var(--wp--preset--font-size--max-72)",
			"creativeColor": "var(--wp--preset--color--contrast)",
			"creativeFont": "var(--wp--preset--font-family--brand)",
			"creativeTextTransform": "none",
			"textShadow": "0 .15rem .3rem rgba(0,0,0, 0.5)",
			"borderRadius": "3px",
			"imageBorderRadius": "var(--wp--custom--border-radius)",
			"buttonBorderRadius": "3px",
			"buttonShadow": "var(--wp--preset--shadow--light)", 
			"buttonHoverShadow": "var(--wp--preset--shadow--dark)",
			"boxShadow": "var(--wp--preset--shadow--light)",
			"boxHoverShadow": "var(--wp--preset--shadow--dark)",
			"transitionTime": "0.35s ease-in-out",	
			"searchMenuIconColor": "var(--wp--preset--color--secondary)",
			"searchMenuIconHoverColor": "var(--wp--preset--color--secondary-dark)",
			"headerSearchColor": "var(--wp--preset--color--contrast)",
			"headerSearchInputBackground": "var(--wp--preset--color--base)",
			"headerSearchButtonPadding": "4px",
			"decoColor": "var(--wp--preset--color--secondary)",
			"gradientAngle": "300deg",
			
			"color":{
				"deco": "var(--wp--preset--color--secondary)",
				"alert": {
					"success": "var(--wp--preset--color--success)",
					"warning": "var(--wp--preset--color--warning)",
					"error": "#DF1642",
					"focus": "blue"
				}
			},
			"spacing": {
				"gap": "var(--wp--preset--spacing--large)"
			},
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"body": 1.75,
				"heading": 1.1,
				"medium": 1.5,
				"one": 1
			}

		},
		"blocks": {
			"core/button": {
				"spacing": {
					"spacingSizes": [
						{
							"name": "xSmall",
							"size": "4px",
							"slug": "x-small"
						},
						{
							"name": "Small",
							"size": "8px",
							"slug": "small"
						},
						{
							"name": "Medium",
							"size": "12px",
							"slug": "medium"
						},
						{
							"name": "Large",
							"size": "16px",
							"slug": "large"
						},
						{
