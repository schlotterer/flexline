	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--gap)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--custom--spacing--gap)",
				"bottom": "0px",
				"left": "var(--wp--custom--spacing--gap)"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"button": {
				"color": {
					"gradient": "none",
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"shadow": "var(--wp--custom--button-shadow)",
				"border": {
					"radius": "var(--wp--custom--button-border-radius)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				":hover": {
					"shadow": "var(--wp--custom--button-hover-shadow)",
					"typography": {
						"textDecoration": "none"
					},
					"color": {
						"gradient": "none",
						"background": "var(--wp--preset--color--secondary-dark)"
					}
				},
				":focus": {
					"outline": {
						"color": "var(--wp--custom--color--alert--focus)"
					}
				},
				"css": "white-space: nowrap;display:block; transition: var(--wp--custom--transition-time);"
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "10px",
						"bottom": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--display)"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "inherit"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--brand)",
					"fontSize": "var(--wp--preset--font-size--max-48)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--brand)",
					"fontSize": "var(--wp--preset--font-size--max-36)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				},
				"css": "text-wrap: balance; & a {color: var(--wp--preset--color--contrast);} & a:hover {color: var(--wp--preset--color--primary);}"
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
