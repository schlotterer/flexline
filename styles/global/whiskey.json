{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
    "title": "Whiskey",
	"description":  "Crimson Text and Jost",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "960px",
			"wideSize": "1380px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Extra Small",
					"size": "12px",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "18px",
					"slug": "small"
				},
				{
					"name": "Medium ( 24px / 32px )",
					"size": "clamp(24px, calc(1.5rem + ((1vw - 7.68px) * 1.5625)), 32px)",
					"slug": "medium"
				},
				{
					"name": "Large ( 32px / 48px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 3.125)), 48px)",
					"slug": "large"
				},
				{
					"name": "Extra Large ( 32px / 64px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 6.25)), 64px)",
					"slug": "x-large"
				},
				{
					"name": "2xl ( 32px / 80px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 9.375)), 80px)",
					"slug": "xx-large"
				},
				{
					"name": "3xl ( 48px / 96px )",
					"size": "clamp(48px, calc(3rem + ((1vw - 7.68px) * 9.375)), 96px)",
					"slug": "xxx-large"
				}			
			],
			"units": [
				"px",
				"em",
				"rem",
				"svh",
				"vh",
				"vw",
				"%"
			]
		},
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--primary-dark) 0%, var(--wp--preset--color--primary) 100%)",
					"name": "Primary to Primary Dark",
					"slug": "primary-primaryDark"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--primary) 0%, var(--wp--preset--color--alternate) 100%)",
					"name": "Primary to Alternate",
					"slug": "primary-alternate"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--primary) 0%, var(--wp--preset--color--highlight) 100%)",
					"name": "Primary to Highlight",
					"slug": "primary-highlight"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--secondary-dark) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Secondary to Secondary Dark",
					"slug": "secondary-secondaryDark"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--primary) 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--alternate) 100%)",
					"name": "Secondary to Alternate",
					"slug": "secondary-alternate"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--highlight) 100%)",
					"name": "Secondary to Highlight",
					"slug": "secondary-highlight"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--alternate-dark) 0%, var(--wp--preset--color--alternate) 100%)",
					"name": "Alternate to Alternate Dark",
					"slug": "alternate-alternateDark"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--alternate) 0%, var(--wp--preset--color--primary) 100%)",
					"name": "Alternate to Primary",
					"slug": "alternate-primary"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--alternate) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Alternate to Secondary",
					"slug": "alternate-secondary"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--alternate) 0%, var(--wp--preset--color--highlight) 100%)",
					"name": "Alternate to Highlight",
					"slug": "alternate-highlight"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--neutral-light) 0%, var(--wp--preset--color--neutral) 100%)",
					"name": "Neutral to Neutral Light",
					"slug": "neutral-neutralLight"
				},
				{
					"gradient": "linear-gradient(var(--wp--custom--gradient-angle), var(--wp--preset--color--neutral-light) 0%, var(--wp--preset--color--neutral-dark) 100%)",
					"name": "Neutral Dark to Neutral Light",
					"slug": "neutralDark-neutralLight"
				},
				{
					"gradient": "linear-gradient(to bottom, var(--wp--preset--color--contrast) 0%, transparent 100%)",
					"name": "Dark to transparent - top to bottom",
					"slug": "dark-transparent-top-to-bottom"
				},
				{
					"gradient": "linear-gradient(to top, var(--wp--preset--color--contrast) 0%, transparent 100%)",
					"name": "Dark to transparent - bottom to top",
					"slug": "dark-transparent-bottom-to-top"
				},
				{
					"gradient": "linear-gradient(to right, var(--wp--preset--color--contrast) 0%, transparent 100%)",
					"name": "Dark to transparent - left to right",
					"slug": "dark-transparent-left-to-right"
				},
				{
					"gradient": "linear-gradient(to left, var(--wp--preset--color--contrast) 0%, transparent 100%)",
					"name": "Dark to transparent - right to left",
					"slug": "dark-transparent-right-to-left"
				}
			],
			"duotone": [
				{
