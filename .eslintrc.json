{
    "extends": ["plugin:@wordpress/eslint-plugin/recommended"],
    "parserOptions": {
        "requireConfigFile": false,
        "babelOptions": {
            "presets": ["@wordpress/babel-preset-default"]
        }
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx"]
            }
        }
    }
}
  
